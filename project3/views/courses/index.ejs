<div class="row">
  <div class="col-sm-6 col-md-4">
    <% for (i = 0; i < courses.length; i ++) { %>
      <div class="thumbnail">
        <img src="<%= courses[i].rating_img_url %>" alt="...">
        <div class="caption">
          <h3><%= courses[i].name %></h3>
          <p><%= courses[i].snippet_text %></p>
          <p><a href="#" class="btn btn-primary" role="button">More Info!</a></p>
        </div>
      </div>
    <% } %>  
  </div>
</div>

</body>
